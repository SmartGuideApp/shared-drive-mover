{
  "compilerOptions": {
    "module": "es6",
    "moduleResolution": "node",
    "forceConsistentCasingInFileNames": true,
    "noFallthroughCasesInSwitch": true,
    "noImplicitReturns": true,
    "noUnusedLocals": true,
    "noUnusedParameters": false,
    "strict": true,
    "resolveJsonModule": true
  }
}
